pipeline {
    agent any

    stages {
        stage('Analylize Build Metrics') {
            steps {
                script {
                    jenkinsDetails = load 'job_metrics/scripts/pull_jenkins_build_details.groovy'
                    jenkinsDetails.constructJobDetailsAPIUrl("http://localhost:8080",params.JOB_NAME,params.JOB_ID)
                }
                
            }
        }
    }
}
